{
  "name": "react-employee-manager",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TressaPark/react-employee-manager.git"
  },
  "author": "Tressa Park",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/TressaPark/react-employee-manager/issues"
  },
  "homepage": "https://github.com/TressaPark/react-employee-manager#readme",
  "dependencies": {
    "express": "^4.18.2",
    "if-env": "^1.0.4"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "nodemon": "^2.0.4"
  }
}
